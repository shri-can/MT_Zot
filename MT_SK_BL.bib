@article{engelComparisonModularMultilevel2015,
  title = {Comparison of the {{Modular Multilevel DC Converter}} and the {{Dual-Active Bridge Converter}} for {{Power Conversion}} in {{HVDC}} and {{MVDC Grids}}},
  author = {Engel, Stefan P. and Stieneker, Marco and Soltau, Nils and Rabiee, Sedigheh and Stagge, Hanno and De Doncker, Rik W.},
