@article{younessImplementationValidationBackstepping2019,
  title = {Implementation and Validation of Backstepping Control for {{PMSG}} Wind Turbine Using {{dSPACE}} Controller Board},
  author = {Youness, El Mourabit and Aziz, Derouich and Abdelaziz, El Ghzizal and Jamal, Bouchnaif and Najib, El Ouanjli and Othmane, Zamzoum and Khalid, Mezioui and Bossoufi, Badre},
  date = {2019-11},
  journaltitle = {Energy Reports},
  shortjournal = {Energy Reports},
  volume = {5},
  pages = {807--821},
  issn = {23524847},
  url = {https://linkinghub.elsevier.com/retrieve/pii/S2352484719301325},
  abstract = {This paper describes the design and implementation in real time of a nonlinear control for a wind energy conversion system (WECS). The Backstepping control has been implemented to improve the performance of the conversion system based on a permanent magnet synchronous generator (PMSG) connected to the grid. Two static back-to-back converters ensure grid connection and are controlled by Pulse Width Modulation (PWM). The proposed control algorithm ensures proper speed control to extract maximum power. First of all, a WECS full review has been discussed. Thereafter, a Backstepping control laws detailed description based on the Lyapunov stability technique has been reported. Consequently, these control thus helping it possible to operate the complete system in the best performances in the static and dynamic regimes. The second part of this article has been devoted to the Backstepping control experimental validation using the dSPACE DS1104 control board and the Matlab–Simulink environment in order to check and validate the system efficiency. The results achieved have been clearly responded to the requirements of robustness and follow-up of references even under fluctuating wind conditions, and confirmed the control effectiveness in both static and dynamic operating modes.},
  langid = {english}
}


